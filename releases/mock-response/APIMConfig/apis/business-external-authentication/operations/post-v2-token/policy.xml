<policies>
    <inbound>
        <base />
        <set-header name="Origin" exists-action="delete" />
        <set-header name="Referer" exists-action="delete" />
        <set-header name="Cookie" exists-action="delete" />
        <set-header name="User-Agent" exists-action="delete" />
    </inbound>
    <backend>
        <base />
    </backend>
    <outbound>
        <base />
    </outbound>
